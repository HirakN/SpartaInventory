
<table class="table table-hover">
  <thead>
  <tr>
    <th>Log id</th>
    <th>Lender</th>
    <th>Borrower</th>
    <th>Item</th>
    <th>Date Taken</th>
    <th>Returned By</th>
    <th>Date Returned</th>
  </tr>
  </thead>

  <tbody>
    <tr>
     <td><%= @log.id %></td>
     <td><%= @log.lender.name %></td>
     <td><%= @log.borrower.name %></td>
     <td><%= @log.item.description %></td>
     <td><%= @log.created_at %></td>
     <td><%= @log.returned_to.try(:name) %></td>
     <td><%= @log.return_date %></td>
    </tr>
  </tbody>
</table>




